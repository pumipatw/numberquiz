   "source": [
    "# ln (x) question\n",
    "def ln(x):\n",
    "    x = 1-x\n",
    "    pre = 0\n",
    "    result = -x\n",
    "    i = 2\n",
    "    while abs(result - pre) > 0.00001:\n",
    "        pre = result\n",
    "        result -= pow(x,i) / i\n",
    "        i = i+1\n",
    "    return result"
   ]
